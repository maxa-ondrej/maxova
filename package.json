{
  "name": "maxova-web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "biome check . && prettier \"./**/*.svelte\" \"./**/*.astro\" --check && eslint --ext .astro src && svelte-check",
    "format": "biome check --apply-unsafe . && biome format . --write && prettier \"./**/*.svelte\" \"./**/*.astro\" --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/svelte": "^4.0.2",
    "@astrojs/tailwind": "^5.0.0",
    "astro": "^3.1.4",
    "lucide-astro": "^0.279.0",
    "sharp": "^0.32.6",
    "svelte": "^4.0.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.2.2",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "svelte-check": "^3.5.2"
  }
}
